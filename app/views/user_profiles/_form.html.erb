<%= simple_form_for @user_profile, {:multipart => true} do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :about %>
    <%= f.input :hobbies %>
    <%= f.input :github %>
    <%= f.input :facebook %>
    <%= f.input :avatar, :as => :file %>
    <%= f.submit 'Create Profile', :class => 'btn btn-success' %>
  
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
